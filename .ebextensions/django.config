option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: fitjeb.settings.prod
  aws:elasticbeanstalk:container:python:
    WSGIPath: fitjeb/wsgi.py

packages:
  yum:  
    git: []
    postgresql93-devel: []
    libjpeg-turbo-devel: []
    libcurl-devel: []
    python34-devel: []

sources:
  /usr/bin: https://bitbucket.org/ariya/phantomjs/downloads/phantomjs-2.1.1-linux-x86_64.tar.bz2
  /usr/bin: https://pypi.python.org/packages/source/p/pycurl/pycurl-7.43.0.tar.gz

container_commands:
  05_migrate:
    command: "django-admin.py migrate"
    leader_only: true
  06_createsu:
    command: "django-admin.py createsu"
    leader_only: true
  07_collectstatic:
    command: "django-admin.py collectstatic --noinput"

